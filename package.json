{
  "name": "react-particles-webgl",
  "version": "1.0.5",
  "description": "A 2D/3D particle library built on React, Three.js and WebGL",
  "author": "Tim Ellenberger <timellenberger@gmail.com>",
  "license": "MIT",
  "repository": "tim-soft/react-particles-webgl",
  "bugs": {
    "url": "https://github.com/tim-soft/react-particles-webgl/issues"
  },
  "homepage": "https://timellenberger.com/particles",
  "keywords": [
    "react",
    "fiber",
    "three",
    "particle",
    "3D",
    "canvas"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cd example && yarn test",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{js, jsx}": [
      "prettier --write",
      "eslint --no-ignore --fix",
      "git add --force"
    ]
  },
  "peerDependencies": {
    "prop-types": ">=15.5.4",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "three": ">=0.100"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "three": "^0.105.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "hex-rgb": "^4.1.0",
    "is-hexcolor": "^1.0.0",
    "react-three-fiber": "^2.2.11",
    "three-orbitcontrols": "^2.102.2"
  }
}
